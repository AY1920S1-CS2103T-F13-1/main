@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":CatalogParser" as CatalogParser LOGIC_COLOR
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR
participant "a:FindCommand" as FindCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
'participant ":Book" as Book MODEL_COLOR
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("find t/Animal Farm a/George")
activate LogicManager

LogicManager -> CatalogParser ++ : parseCommand("find t/Animal Farm a/George")

create FindCommandParser
CatalogParser -> FindCommandParser
activate FindCommandParser

FindCommandParser --> CatalogParser
deactivate FindCommandParser

CatalogParser -> FindCommandParser : parse("t/Animal Farm a/George")
activate FindCommandParser

create FindCommand
FindCommandParser -> FindCommand ++
return a

FindCommandParser --> CatalogParser : a
deactivate FindCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
FindCommandParser -[hidden]-> CatalogParser
destroy FindCommandParser

return a

LogicManager -> FindCommand ++ : execute()

FindCommand -> Model ++: updateFilteredList()
return

create CommandResult
FindCommand -> CommandResult ++
return

return result

[<--LogicManager
deactivate LogicManager

@enduml
