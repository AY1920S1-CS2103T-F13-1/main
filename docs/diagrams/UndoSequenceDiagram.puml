@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":CatalogParser" as CatalogParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedCatalog" as VersionedCatalog MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> CatalogParser : parseCommand(undo)
activate CatalogParser

create UndoCommand
CatalogParser -> UndoCommand
activate UndoCommand

UndoCommand --> CatalogParser
deactivate UndoCommand

CatalogParser --> LogicManager : u
deactivate CatalogParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoCatalog()
activate Model

Model -> VersionedCatalog : undo()
activate VersionedCatalog

VersionedCatalog --> Model :
deactivate VersionedCatalog

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
